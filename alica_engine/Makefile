# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  alica_engine_config = debug

else ifeq ($(config),release)
  alica_engine_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := alica_engine

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

alica_engine:
ifneq (,$(alica_engine_config))
	@echo "==== Building alica_engine ($(alica_engine_config)) ===="
	@${MAKE} --no-print-directory -C . -f alica_engine.make config=$(alica_engine_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f alica_engine.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   alica_engine"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"