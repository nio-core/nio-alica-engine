# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  alica_common_config_config = debug

else ifeq ($(config),release)
  alica_common_config_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := alica_common_config

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

alica_common_config:
ifneq (,$(alica_common_config_config))
	@echo "==== Building alica_common_config ($(alica_common_config_config)) ===="
	@${MAKE} --no-print-directory -C . -f alica_common_config.make config=$(alica_common_config_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f alica_common_config.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   alica_common_config"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"